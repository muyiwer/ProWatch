    <script src="~/PorWatchFrontEnd/Layout/jquery.knob.min.js"></script>
    <script src="~/PorWatchFrontEnd/Layout/moment.min.js"></script>
    <script src="~/PorWatchFrontEnd/Layout/daterangepicker.js"></script>
    <script src="~/PorWatchFrontEnd/Layout/bootstrap-datepicker.min.js"></script>
    <script src="~/PorWatchFrontEnd/Layout/bootstrap3-wysihtml5.all.min.js"></script>
    <script src="~/PorWatchFrontEnd/Layout/jquery.slimscroll.min.js"></script>
    <script src="~/PorWatchFrontEnd/Layout/fastclick.js"></script>
    <script src="~/PorWatchFrontEnd/Layout/adminlte.min.js"></script>
    <script src="~/PorWatchFrontEnd/Layout/dashboard.js"></script>
    <script src="~/PorWatchFrontEnd/Layout/demo.js"></script>
    <script src="~/PorWatchFrontEnd/Layout/bootstrap.min.js"></script>




    <script src="~/PorWatchFrontEnd/js/semantic.min.js"></script>   
    <script src="~/PorWatchFrontEnd/js/chart-data.js"></script>
    <script src="~/PorWatchFrontEnd/js/custom.js"></script>
    <script src="~/PorWatchFrontEnd/js/easypiechart.js"></script>
    <script src="~/PorWatchFrontEnd/js/easypiechart-data.js"></script>
    
    <script src="~/PorWatchFrontEnd/js/chart.min.js"></script>



  

</body>
</html>



