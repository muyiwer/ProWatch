
@{
    ViewBag.Title = "UploadFile";
    Layout = "~/Views/Shared/_ProWatchLayout.cshtml";
}

<h2>UploadFile</h2>
@using (Html.BeginForm("UploadFile", "projects", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
<div class="form-group col-md-8">
    <label>File input</label>
    <input type="file" name="file" accept="image/*"/>
</div>
<div class="form-group col-md-8">
    <button id="ken" type="submit" class="btn btn-primary" style="float:left">Upload</button>
</div>
}